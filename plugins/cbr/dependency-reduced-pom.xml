<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.gillsoft.rate</groupId>
  <artifactId>gds-rate-cbr</artifactId>
  <version>0.0.1</version>
  <build>
    <finalName>gds-rate-cbr</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>${jdk.version}</source>
          <target>${jdk.version}</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>com.gillsoft</groupId>
      <artifactId>gds-rate-commons</artifactId>
      <scope>system</scope>
      <systemPath>C:\git\gds-rate\gds-rate-commons\target\gds-rate-commons-0.0.1.jar</systemPath>
    </dependency>
  </dependencies>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.gillsoft</groupId>
        <artifactId>gds-rate-commons</artifactId>
        <version>0.0.1</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <properties>
    <spring.version>5.0.6.RELEASE</spring.version>
    <jdk.version>1.8</jdk.version>
    <log4j.version>2.11.0</log4j.version>
    <jackson.version>2.9.5</jackson.version>
  </properties>
</project>
